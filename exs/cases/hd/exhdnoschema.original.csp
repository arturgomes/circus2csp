-----------------------------------------------------------------------
-----------------------------------------------------------------------
-------------   Circus Specification - HD Machine   -------------
-----------------------------------------------------------------------
-----------------------------------------------------------------------

-----------------------------------------------------------------------
-------------   1.1 Types and state variables     -------------
-----------------------------------------------------------------------

HOUR = {0..1}
MINUTE = {0..1}
SECOND = {0..1}
TIME = (HOUR,MINUTE)
INTEGER = {0..1}
CheckInterval = 1
datatype BUTTON = ON | OFF
datatype SWITCH = ENABLED | DISABLED
datatype DIRECTION = FORWARD | BACKWARDS
datatype STATEPHASE = connectThePatient | initPhase
           | prepPhase | endPhase
datatype ACTIVITY = rinsingEBC | applicationArterialBolus
          | anticoagDelivRun | reinfProcess
          | preparationOfDF | priming
          | rinsing | idle
datatype LID = OPEN | CLOSED
datatype CONCENTRATE = ACID | ACETATE | BICARBONATE
datatype HDMODE = RUN | ALARM | BYPASS
datatype LAMP = GREEN | YELLOW
datatype BOOL = TRUE | FALSE

-----------------------------------------------------------------------
-------------      1.2 State Components      -------------
-----------------------------------------------------------------------

-- HDGenComp components:
--  airVolLimit : INTEGER
--  airVol : INTEGER
--  alarm : SWITCH
--  apTransdPress : INTEGER
--  artBolusVol : INTEGER
--  bloodFlowInEBC : INTEGER
--  bloodLines : BOOL
--  bypassValve : LID
--  fflowDirect : DIRECTION
--  hdActivity : \power ACTIVITY
--  hdMachineState : \power STATEPHASE
--  hdMode : HDMODE
--  infSalineVol : INTEGER
--  lastNonZeroBF : INTEGER
--  lowerPressureLimit : INTEGER
--  minUFRateTreat : BUTTON
--  netFluidRemoval : SWITCH
--  netFluidRemovalRate : INTEGER
--  rotDirectBP : DIRECTION
--  rotDirectUFP : DIRECTION
--  sadSensorFlow : INTEGER
--  safeUpperLimit : INTEGER
--	 signalLamp : LAMP
--  timerINTEGERervalR10 : INTEGER
--  timerINTEGERervalR11 : INTEGER
--  timerINTEGERervalR12 : INTEGER
--  timerINTEGERervalR13 : INTEGER
--  timerINTEGERervalR9 : INTEGER
--  upperPressureLimit : INTEGER
--  volumeInEBC : INTEGER
--  vpTransdPress : INTEGER

nametype HDGenComp =
		(INTEGER, INTEGER, SWITCH, INTEGER, INTEGER,
		INTEGER, LID, DIRECTION, Set(ACTIVITY), Set(STATEPHASE),
		HDMODE, INTEGER, INTEGER, INTEGER, INTEGER, SWITCH,
		DIRECTION, DIRECTION, INTEGER, INTEGER, LAMP,
		INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER,
		INTEGER, BOOL, BUTTON)



