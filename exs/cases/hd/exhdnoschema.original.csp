-------------         1.5 Clock          -------------
-----------------------------------------------------------------------
---------------------------------------------------
-------------    Tock-CSP     -------------
---------------------------------------------------

channel tock, timeout
RUNNING(X) =
	[] x:X @ x -> RUNNING(X)
TOCKS =
	tock -> TOCKS

Wait(n) =
	if n>0 then tock -> Wait(n-1) else SKIP

Timeout(A,B,t) =
	let Counter(t) =
		if (t>0)
		then tock -> Counter(t-1)
		else timeout -> SKIP
	within ((A /\ timeout -> B) [|{|timeout,tock|}|] Counter(t)) \ {|timeout|}

channel setTime, getTime : {0..1}

------------------------------------------
--------- Clock definitions --------------
SysClock =
	let

	ClockSt(ms) =
		getTime!ms -> ClockSt(ms)
		[] setTime?x -> ClockSt(x)

	Clock =
		let X =
		(((getTime?clock -> (Wait(1) ;
				if clock == 1
				then tock -> setTime!(0) -> SKIP
				else tock -> setTime!(clock + 1) -> SKIP))
		||| (Timeout((let Y =
		getTime?clock -> getCurrentTime!clock -> Y within Y),SKIP,1))
		); X)
		within X

	ResetClock =
		setTime!0 -> Clock
	within (ClockSt(0)
			[|{|getTime,setTime,tock|}|]

	ResetClock) /\ therapyEnding -> SKIP
