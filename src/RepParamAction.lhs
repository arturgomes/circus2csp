  = (CSPRenAction _ZName (repParamActionCReplace x y _CReplace))
repParamActionCAction x y (CSPRecursion _ZName _CAction)
  = (CSPRecursion _ZName (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPUnfAction _ZName _CAction)
  = (CSPUnfAction _ZName (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPUnParAction _ZGenFilt_lst _CAction _ZName)
  = (CSPUnParAction (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction) _ZName)
repParamActionCAction x y (CSPRepSeq _ZGenFilt_lst _CAction)
  = (CSPRepSeq (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPRepExtChoice _ZGenFilt_lst _CAction)
