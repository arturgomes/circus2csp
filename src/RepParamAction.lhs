  = (CAssumpt _ZName_lst (repParamActionZPred  x y _ZPred1) (repParamActionZPred  x y _ZPred2))
repParamActionCCommand x y (CAssumpt1 _ZName_lst _ZPred)
  = (CAssumpt1 _ZName_lst (repParamActionZPred  x y _ZPred))
repParamActionCCommand x y (CPrefix _ZPred1 _ZPred2)
  = (CPrefix (repParamActionZPred  x y _ZPred1) (repParamActionZPred  x y _ZPred2))
repParamActionCCommand x y (CPrefix1 _ZPred)
  = (CPrefix1 (repParamActionZPred  x y _ZPred))
repParamActionCCommand x y (CommandBrace _ZPred)
  = (CommandBrace (repParamActionZPred  x y _ZPred))
repParamActionCCommand x y (CommandBracket _ZPred)
  = (CommandBracket (repParamActionZPred  x y _ZPred))
