\end{code}
\subsection{\Circus\ Commands}
\begin{code}
repParamActionCCommand x y (CAssign _ZVar_lst _ZExpr_lst)
  = (CAssign (map (repParamActionZVar x y) _ZVar_lst) (map (repParamActionZExpr x y) _ZExpr_lst))
repParamActionCCommand x y (CIf _CGActions)
  = (CIf (repParamActionCGActions x y _CGActions))
repParamActionCCommand x y (CVarDecl _ZGenFilt_lst _CAction)
  = (CVarDecl (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction))
