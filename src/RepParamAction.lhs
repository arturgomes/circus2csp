\subsection{\Circus\ Guards}
\begin{code}
repParamActionCGActions x y (CircGAction _ZPred _CAction)
  = (CircGAction (repParamActionZPred x y _ZPred) (repParamActionCAction x y _CAction))
repParamActionCGActions x y (CircThenElse _CGActions _CGActions1)
  = (CircThenElse (repParamActionCGActions x y _CGActions) (repParamActionCGActions x y _CGActions1))
\end{code}
\subsection{Circus Renaming}
\begin{code}
repParamActionCReplace x y (CRename _ZVar_lst1 _ZVar_lst2)
  = (CRename (map (repParamActionZVar x y) _ZVar_lst1) (map (repParamActionZVar x y) _ZVar_lst2))
repParamActionCReplace x y (CRenameAssign _ZVar_lst1 _ZVar_lst2)
  = (CRenameAssign (map (repParamActionZVar x y) _ZVar_lst1) (map (repParamActionZVar x y) _ZVar_lst2))
\end{code}


\begin{code}
repParamActionZVar x y _ZVar = _ZVar
\end{code}


\begin{code}
repParamActionZExpr x y (ZVar _ZVar)
