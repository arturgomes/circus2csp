\begin{code}
module RepParamAction where

import AST
\end{code}

\subsection{Process paragraphs}

\begin{code}
repParamActionPPar x y (ProcZPara _ZPara)
  = (ProcZPara _ZPara)
repParamActionPPar x y (CParAction _ZName _ParAction)
  = (CParAction _ZName (repParamActionParAction x y _ParAction))
repParamActionPPar x y (CNameSet _ZName _ZExpr_lst)
  = (CNameSet _ZName (map (repParamActionZExpr x y) _ZExpr_lst))
\end{code}
\subsection{Parametrised Actions}
\begin{code}
repParamActionParAction x y (CircusAction _CAction)
  = (CircusAction (repParamActionCAction x y _CAction))
repParamActionParAction x y (ParamActionDecl _ZGenFilt_lst _ParAction)
  = (ParamActionDecl (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionParAction x y _ParAction))
\end{code}
\subsection{\Circus\ Actions}
\begin{code}
repParamActionCAction x y (CActionSchemaExpr _ZSExpr)
  = (CActionSchemaExpr _ZSExpr)
repParamActionCAction x y (CActionCommand _CCommand)
  = (CActionCommand (repParamActionCCommand x y _CCommand))
repParamActionCAction x y (CActionName _ZName)
  = (CActionName _ZName)
repParamActionCAction x y (CSPSkip )
  = (CSPSkip )
repParamActionCAction x y (CSPStop )
  = (CSPStop )
repParamActionCAction x y (CSPChaos)
  = (CSPChaos)
repParamActionCAction x y (CSPCommAction _Comm _CAction)
  = (CSPCommAction _Comm (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPGuard _ZPred _CAction)
  = (CSPGuard (repParamActionZPred  x y _ZPred) (repParamActionCAction x y _CAction))
