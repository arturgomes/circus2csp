\subsection{\Circus\ Communication}
\begin{code}
repParamActionComm x y (ChanComm _ZName _CParameter_lst)
  = (ChanComm _ZName (map (repParamActionCParameter x y) _CParameter_lst))
repParamActionComm x y (ChanGenComm _ZName _ZExpr_lst _CParameter_lst)
  = (ChanGenComm _ZName (map (repParamActionZExpr x y) _ZExpr_lst) (map (repParamActionCParameter x y) _CParameter_lst))
\end{code}
\subsection{\Circus\ Communication}
\begin{code}
repParamActionCParameter x y (ChanInp _ZName)
  = (ChanInp _ZName)
repParamActionCParameter x y (ChanInpPred _ZName _ZPred)
  = (ChanInpPred _ZName (repParamActionZPred  x y _ZPred))
repParamActionCParameter x y (ChanOutExp _ZExpr)
