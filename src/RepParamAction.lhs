repParamActionCAction x y (CSPRepIntChoice _ZGenFilt_lst _CAction)
  = (CSPRepIntChoice (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPRepParalNS _CSExp _ZGenFilt_lst _ZExpr_lst _CAction)
  = (CSPRepParalNS _CSExp (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (map (repParamActionZExpr x y) _ZExpr_lst) (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPRepParal _CSExp _ZGenFilt_lst _CAction)
  = (CSPRepParal _CSExp (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPRepInterlNS _ZGenFilt_lst _ZExpr_lst _CAction)
  = (CSPRepInterlNS (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (map (repParamActionZExpr x y) _ZExpr_lst) (repParamActionCAction x y _CAction))
repParamActionCAction x y (CSPRepInterl _ZGenFilt_lst _CAction)
  = (CSPRepInterl (map (repParamActionZGenFilt x y) _ZGenFilt_lst) (repParamActionCAction x y _CAction))
